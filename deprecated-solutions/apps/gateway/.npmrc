# npm configuration for Gateway
# Package manager: npm (package-lock.json)
# Lockfile version 3 for npm 7+
lockfile-version=3

# Disable funding messages in CI
fund=false

# Disable audit in development (enable in CI via npm ci --audit)
audit=false

# Save exact versions (not ranges) for reproducible builds
save-exact=false

# Use package-lock.json (not shrinkwrap)
package-lock=true
